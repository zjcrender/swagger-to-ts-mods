{
  "compilerOptions": {
    "target": "es6",
    "lib": [
      "esnext"
    ],
    "module": "commonjs",
    "baseUrl": "./",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "skipLibCheck": true,
    "isolatedModules": true
  },
  "include": [
    "src/**/*",
  ]
}
